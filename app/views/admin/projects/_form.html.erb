<%= semantic_form_for [:admin, resource] do |f| %>
  <% if resource.persisted? %>
    <%= f.inputs :full_name, :youtrack_url, :youtrack_token, :workflow_name, :jira_url %>
  <% else %>
    <%= f.inputs :id, :full_name, :youtrack_url, :youtrack_token, :workflow_name, :jira_url %>
  <% end %>
  <%= f.button :commit %>
<% end %>
